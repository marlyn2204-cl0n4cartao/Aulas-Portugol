programa {
  funcao inicio() {
    // Declaração de Variáveis
     cadeia nome, sobrenome, nomeCompleto
    // Entrada de dados 
        escreva("Digite o primeiro nome: ")
        leia(nome)

        escreva("Digite o sobrenome: ")
        leia(sobrenome)
    // Processamento de dados 
        nomeCompleto = nome + " " + sobrenome
    // Saida de dados
        escreva("Nome completo: ", nomeCompleto, "\n")
  }
}
